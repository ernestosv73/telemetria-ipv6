{
  "title": "ACL Matched Packets Overview",
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Matched Packets per ACL Entry",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "acl_interface_input_acl_filter_entry_statistics_matched_packets{entry_sequence_id=\"5\"}",
          "legendFormat": "iface {{interface_interface_id}} RAGuard",
          "refId": "A"
        },
        {
          "expr": "acl_interface_input_acl_filter_entry_statistics_matched_packets{entry_sequence_id=\"6\"}",
          "legendFormat": "iface {{interface_interface_id}} DenyScan",
          "refId": "B"
        },
        {
          "expr": "acl_interface_input_acl_filter_entry_statistics_matched_packets{entry_sequence_id=\"100\"}",
          "legendFormat": "iface {{interface_interface_id}} DenyICMPv6Attack",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "pps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "options": {
        "tooltip": {
          "mode": "all"
        },
        "legend": {
          "displayMode": "table",
          "showLegend": true,
          "placement": "bottom"
        }
      }
    },
    {
      "type": "gauge",
      "title": "Multicast Packets (per Interface)",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "interface_statistics_in_multicast_packets",
          "legendFormat": "{{interface_interface_id}}",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "pps",
          "min": 0,
          "max": 2000,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 500
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Platform CPU Usage (Instant)",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "platform_control_cpu_total_instant",
          "legendFormat": "{{source}}",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "orange", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "all"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 14
      }
    }
  ],
  "schemaVersion": 41,
  "version": 1,
  "uid": "acl-dashboard",
  "annotations": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}
